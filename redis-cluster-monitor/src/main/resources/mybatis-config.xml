	<mapper resource="com/linda/cluster/redis/monitor/dao/sqlmap/MonitorMemoryDaoSqlmap.xml"/>
	<mapper resource="com/linda/cluster/redis/monitor/dao/sqlmap/MonitorPersistenceDaoSqlmap.xml"/>
	<mapper resource="com/linda/cluster/redis/monitor/dao/sqlmap/MonitorReplicationDaoSqlmap.xml"/>
	<mapper resource="com/linda/cluster/redis/monitor/dao/sqlmap/MonitorServerDaoSqlmap.xml"/>
