	<typeAlias alias="monitorMemory" type="com.linda.cluster.redis.monitor.pojo.MonitorMemory"/>
	<typeAlias alias="monitorPersistence" type="com.linda.cluster.redis.monitor.pojo.MonitorPersistence"/>
	<typeAlias alias="monitorReplication" type="com.linda.cluster.redis.monitor.pojo.MonitorReplication"/>
