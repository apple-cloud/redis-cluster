<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

<typeAliases>
    <typeAlias alias="cluster" type="com.linda.cluster.redis.monitor.pojo.Cluster"/>
    <typeAlias alias="product" type="com.linda.cluster.redis.monitor.pojo.Product"/>
	<typeAlias alias="redisNode" type="com.linda.cluster.redis.monitor.pojo.RedisNode"/>
	
    <typeAlias alias="monitorClients" type="com.linda.cluster.redis.monitor.pojo.MonitorClients"/>
